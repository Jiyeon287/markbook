<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 <mapper namespace="com.markbook.mapper.memberMapper">

	<!-- 아이디 중복체크 -->
	<select id="idChk" resultType="Boolean">
		select count(*) 
		from member_info
		where m_id = #{m_id}
	</select>
	
 </mapper>
